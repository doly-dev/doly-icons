{
  "name": "doly-icons",
  "version": "1.5.0",
  "description": "React icon component based on Bootstrap Icons",
  "repository": "git@github.com:doly-dev/doly-icons.git",
  "keywords": [
    "react",
    "doly",
    "icons",
    "svg",
    "bootstrap",
    "bootstrap-icons"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs-version:build": "cross-env BUIDL_DOC_VERSION=version npm run docs:build",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "rm -rf lib && tsc -p tsconfig.build.json --outDir lib -t ES5 -m commonjs",
    "build:es": "rm -rf es && tsc -p tsconfig.build.json --outDir es -t ES5 -m ES2015",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm test && npm run build",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint-fix:js && npm run lint-fix:style",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:stylelint -- --fix",
    "commit": "cz",
    "lint-staged": "lint-staged",
    "generator": "npm run generator:assets && npm run generator:icons && npm run generator:data",
    "generator:assets": "node scripts/generator-assets.js",
    "generator:icons": "node scripts/generator-icons.js",
    "generator:data": "node scripts/generator-data.js",
    "tsc": "tsc --noEmit"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "sideEffects": false,
  "files": [
    "es",
    "lib"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "npx --no -- commitlint --edit \"$1\""
  },
  "lint-staged": {
    "*.less": [
      "stylelint --fix"
    ],
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.3.2",
    "tslib": "^2.6.0"
  },
  "peerDependencies": {
    "react": ">=16.12.0"
  },
  "devDependencies": {
    "@babel/core": "~7.21.5",
    "@babel/preset-env": "~7.21.5",
    "@babel/preset-typescript": "~7.21.5",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react-color": "^3.0.6",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-helmet": "^6.1.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@umijs/fabric": "^3.0.0",
    "antd": "^5.7.2",
    "antd-more": "^4.1.36",
    "babel-jest": "^29.5.0",
    "bootstrap-icons": "1.10.4",
    "cache2": "^1.0.4",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "copy-image-clipboard": "^2.1.2",
    "copy-to-clipboard": "^3.3.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.49",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^10.1.0",
    "glob": "^10.2.2",
    "iconv-lite": "^0.6.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-two-style-order": "^1.0.1",
    "rc-hooks": "^3.0.3",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-test-renderer": "^18.2.0",
    "react-window": "^1.8.9",
    "save-svg-as-png": "^1.4.17",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.6",
    "ut2": "^1.3.2",
    "util-helpers": "^4.19.3",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "@babel/helper-compilation-targets": "~7.21.5"
  }
}
